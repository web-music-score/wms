{
    "name": "@tspro/web-music-score",
    "version": "1.1.0",
    "author": "PahkaSoft",
    "license": "MIT",
    "private": false,
    "bugs": {
        "url": "https://github.com/pahkasoft/issues/issues/new"
    },
    "description": "TS/JS component for rendering and playing music scores in the browser.",
    "types": "./dist/index.d.ts",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "exports": {
        ".": {
            "browser": {
                "require": {
                    "types": "./dist/index.d.ts",
                    "default": "./dist/index.js"
                },
                "import": {
                    "types": "./dist/index.d.mts",
                    "default": "./dist/index.mjs"
                }
            },
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            }
        }
    },
    "unpkg": "./dist/index.global.js",
    "dependencies": {
        "@tspro/ts-utils-lib": "^1.1.0",
        "tone": "^15.1.22"
    },
    "peerDependencies": {
        "react": "^18 || ^19"
    },
    "devDependencies": {
        "@tspro/tsup-plugin-inline-assets": "^1.0.0",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "tsup": "^8.5.0",
        "typedoc": "^0.28.6",
        "typescript": "latest"
    },
    "workspaces": [
        ".",
        "./apps/*"
    ],
    "scripts": {
        "build": "tsup",
        "watch": "tsup --watch",
        "build:dev (all workspaces)": "npm run build && npm run build:dev --workspaces --if-present",
        "build:prod (all workspaces)": "npm run build && npm run build:prod --workspaces --if-present",
        "docs": "typedoc"
    }
}