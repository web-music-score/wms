{
    "name": "web-music-score",
    "version": "6.4.0-pre.1",
    "author": "Stefan B.",
    "license": "MIT",
    "private": false,
    "homepage": "https://web-music-score.org",
    "repository": {
        "type": "git",
        "url": "https://github.com/web-music-score/wms.git"
    },
    "bugs": {
        "url": "https://github.com/web-music-score/wms/issues/new/choose"
    },
    "description": "Web Music Score is a TypeScript/JavaScript music notation web component and player.",
    "exports": {
        "./core": {
            "require": {
                "types": "./dist/core/index.d.ts",
                "default": "./dist/core/index.js"
            },
            "import": {
                "types": "./dist/core/index.d.ts",
                "default": "./dist/core/index.mjs"
            }
        },
        "./audio": {
            "require": {
                "types": "./dist/audio/index.d.ts",
                "default": "./dist/audio/index.js"
            },
            "import": {
                "types": "./dist/audio/index.d.ts",
                "default": "./dist/audio/index.mjs"
            }
        },
        "./audio-synth": {
            "require": {
                "types": "./dist/audio-synth/index.d.ts",
                "default": "./dist/audio-synth/index.js"
            },
            "import": {
                "types": "./dist/audio-synth/index.d.ts",
                "default": "./dist/audio-synth/index.mjs"
            }
        },
        "./audio-cg": {
            "require": {
                "types": "./dist/audio-cg/index.d.ts",
                "default": "./dist/audio-cg/index.js"
            },
            "import": {
                "types": "./dist/audio-cg/index.d.ts",
                "default": "./dist/audio-cg/index.mjs"
            }
        },
        "./theory": {
            "require": {
                "types": "./dist/theory/index.d.ts",
                "default": "./dist/theory/index.js"
            },
            "import": {
                "types": "./dist/theory/index.d.ts",
                "default": "./dist/theory/index.mjs"
            }
        },
        "./score": {
            "require": {
                "types": "./dist/score/index.d.ts",
                "default": "./dist/score/index.js"
            },
            "import": {
                "types": "./dist/score/index.d.ts",
                "default": "./dist/score/index.mjs"
            }
        },
        "./react-ui": {
            "require": {
                "types": "./dist/react-ui/index.d.ts",
                "default": "./dist/react-ui/index.js"
            },
            "import": {
                "types": "./dist/react-ui/index.d.ts",
                "default": "./dist/react-ui/index.mjs"
            }
        },
        "./pieces": {
            "require": {
                "types": "./dist/pieces/index.d.ts",
                "default": "./dist/pieces/index.js"
            },
            "import": {
                "types": "./dist/pieces/index.d.ts",
                "default": "./dist/pieces/index.mjs"
            }
        }
    },
    "unpkg": "./dist/iife/index.js",
    "jsdelivr": "./dist/iife/index.js",
    "files": [
        "CHANGELOG.md",
        "LICENSE",
        "README.md",
        "dist/**/*.js",
        "dist/**/*.mjs",
        "dist/**/*.d.ts",
        "package.json"
    ],
    "peerDependencies": {
        "react": "^18.0.0 || ^19.0.0"
    },
    "devDependencies": {
        "@tspro/tsup-plugin-inline-assets": "^1.0.0",
        "@tspro/ts-utils-lib": "^3.3.0",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "color-name-to-code": "^1.0.1",
        "rimraf": "^6.0.1",
        "tone": "^15.1.22",
        "tsup": "^8.5.0",
        "typedoc": "^0.28.13",
        "typescript": "latest"
    },
    "workspaces": [
        ".",
        "./apps/*"
    ],
    "scripts": {
        "build": "tsup",
        "watch": "tsup --watch",
        "build:all": "npm run build && npm run build:prod --workspaces --if-present",
        "start:features-demo": "npm run start --workspace features-demo",
        "start:cjs-react-example": "npm run start --workspace js-react-example",
        "start:ts-example": "npm run start --workspace ts-example",
        "start:ts-react-example": "npm run start --workspace ts-react-example",
        "docs": "typedoc"
    }
}