{
    "name": "@tspro/web-music-score",
    "version": "3.1.1",
    "author": "PahkaSoft",
    "license": "MIT",
    "private": false,
    "bugs": {
        "url": "https://github.com/pahkasoft/issues/issues/new"
    },
    "description": "TS/JS component for rendering and playing music scores in the browser.",
    "exports": {
        "./core": {
            "require": {
                "types": "./dist/core/index.d.ts",
                "default": "./dist/core/index.js"
            },
            "import": {
                "types": "./dist/core/index.d.mts",
                "default": "./dist/core/index.mjs"
            }
        },
        "./audio": {
            "require": {
                "types": "./dist/audio/index.d.ts",
                "default": "./dist/audio/index.js"
            },
            "import": {
                "types": "./dist/audio/index.d.mts",
                "default": "./dist/audio/index.mjs"
            }
        },
        "./audio-cg": {
            "require": {
                "types": "./dist/audio-cg/index.d.ts",
                "default": "./dist/audio-cg/index.js"
            },
            "import": {
                "types": "./dist/audio-cg/index.d.mts",
                "default": "./dist/audio-cg/index.mjs"
            }
        },
        "./theory": {
            "require": {
                "types": "./dist/theory/index.d.ts",
                "default": "./dist/theory/index.js"
            },
            "import": {
                "types": "./dist/theory/index.d.mts",
                "default": "./dist/theory/index.mjs"
            }
        },
        "./score": {
            "require": {
                "types": "./dist/score/index.d.ts",
                "default": "./dist/score/index.js"
            },
            "import": {
                "types": "./dist/score/index.d.mts",
                "default": "./dist/score/index.mjs"
            }
        },
        "./react-ui": {
            "require": {
                "types": "./dist/react-ui/index.d.ts",
                "default": "./dist/react-ui/index.js"
            },
            "import": {
                "types": "./dist/react-ui/index.d.mts",
                "default": "./dist/react-ui/index.mjs"
            }
        },
        "./pieces": {
            "require": {
                "types": "./dist/pieces/index.d.ts",
                "default": "./dist/pieces/index.js"
            },
            "import": {
                "types": "./dist/pieces/index.d.mts",
                "default": "./dist/pieces/index.mjs"
            }
        }
    },
    "unpkg": "./dist/iife/index.global.js",
    "files": [
        "CHANGELOG.md",
        "LICENSE",
        "README.md",
        "dist/**/*.js",
        "dist/**/*.mjs",
        "dist/**/*.d.ts",
        "dist/**/*.d.mts",
        "package.json"
    ],
    "dependencies": {
        "@tspro/ts-utils-lib": "^1.3.0",
        "tone": "^15.1.22"
    },
    "peerDependencies": {
        "react": "^18.0.0 || ^19.0.0"
    },
    "devDependencies": {
        "@tspro/tsup-plugin-inline-assets": "^1.0.0",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "tsup": "^8.5.0",
        "typedoc": "^0.28.6",
        "typescript": "latest"
    },
    "workspaces": [
        ".",
        "./apps/*"
    ],
    "scripts": {
        "build": "tsup",
        "watch": "tsup --watch",
        "build:dev (all workspaces)": "npm run build && npm run build:dev --workspaces --if-present",
        "build:prod (all workspaces)": "npm run build && npm run build:prod --workspaces --if-present",
        "docs": "typedoc"
    }
}