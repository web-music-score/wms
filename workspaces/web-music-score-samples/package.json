{
    "name": "web-music-score-samples",
    "version": "1.0.0",
    "author": "Stefan B.",
    "license": "MIT",
    "private": false,
    "homepage": "https://web-music-score.org",
    "repository": {
        "type": "git",
        "url": "https://github.com/web-music-score/wms.git"
    },
    "bugs": {
        "url": "https://github.com/web-music-score/wms/issues/new/choose"
    },
    "description": "This package contains instrument samples for Web Music Score library.",
    "exports": {
        "./core": {
            "require": {
                "types": "./dist/core/index.d.ts",
                "default": "./dist/core/index.js"
            },
            "import": {
                "types": "./dist/core/index.d.ts",
                "default": "./dist/core/index.mjs"
            }
        },
        "./classical-guitar": {
            "require": {
                "types": "./dist/classical-guitar/index.d.ts",
                "default": "./dist/classical-guitar/index.js"
            },
            "import": {
                "types": "./dist/classical-guitar/index.d.ts",
                "default": "./dist/classical-guitar/index.mjs"
            }
        }
    },
    "files": [
        "CHANGELOG.md",
        "LICENSE",
        "README.md",
        "dist/**/*.js",
        "dist/**/*.mjs",
        "dist/**/*.d.ts",
        "package.json"
    ],
    "devDependencies": {
        "@tspro/tsup-plugin-inline-assets": "^1.0.3",
        "tsup": "^8.5.0",
        "typescript": "latest"
    },
    "scripts": {
        "build": "tsup"
    }
}